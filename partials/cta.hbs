<div
  class="post-cta"
  data-keywords="{{#if metaKeywords}}{{toLowerCase metaKeywords}}{{else}}elasticsearch{{/if}}"
>
  <div class="post-cta__text"></div>
  <div class="post-cta__actions">
    <a
      class="button-filled"
      href="#contactForm"
      style="min-width: 218px"
      data-gtm="BlogContactButton"
    >
      Contact Us
    </a>
    <button
      class="button-outline"
      type="button"
      data-bs-toggle="modal"
      data-bs-target="#calendlyModal"
      style="min-width: 218px"
      data-gtm="BlogCalendlyButton"
    >
      Schedule a Meeting
    </button>
  </div>
  {{> modals/calendly-modal}}
</div>
