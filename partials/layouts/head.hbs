<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
{{#if (or (eq template "tagged") (eq template "series") (eq template "posts")) }}
<meta name="robots" content="noindex, follow">
{{/if}}

<title>{{#if title}}{{title}}{{else}}{{blogDescription}}{{/if}} - {{blogTitle}}</title>

{{#if description}}
    <meta name="description" content="{{description}}">
{{/if}}
{{#if metaKeywords}}
    <meta name="keywords" content="{{metaKeywords}}">
{{/if}}
{{#if canonicalUrl}}
    <link rel="canonical" href="{{canonicalUrl}}" />
{{/if}}
<meta http-equiv="Content-Security-Policy" content="default-src {{#if isLocalhost}}http:{{else}}https:{{/if}} 'unsafe-inline';connect-src wss: {{#if isLocalhost}}http:{{else}}https:{{/if}}">
{{#each og}}
    {{#if this.content}}
        <meta property="{{this.property}}" content="{{this.content}}" />
    {{/if}}
{{/each}}

